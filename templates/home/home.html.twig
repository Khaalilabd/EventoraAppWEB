                                        {% else %}
                                            <li><a href="{{ path('discord_login') }}">Connexion avec Discord</a></li>
                                            <li><a href="{{ path('app_auth') }}">Connexion</a></li>
                                            <li><a href="{{ path('app_auth') }}">Inscription</a></li>
                                        {% endif %}
                                    </ul>
                                </li>
                            </ul>
                        </div>
                    </div>
                </nav>

                <header id="fh5co-header" class="fh5co-cover" role="banner">
                    <div class="splitview skewed">
                        <div class="panel bottom">
                            <div class="content">
                                <div class="description">
                                    <h1>The original image.</h1>
                                    <p>This is how the image looks like before applying a duotone effect.</p>
                                </div>
                                <img src="https://i.pinimg.com/736x/e2/1d/24/e21d24e61e8554d6af3c1aa37508bc89.jpg" alt="Original">
                            </div>
                        </div>
                        <div class="panel top">
                            <div class="content">
                                <div class="description">
                                    <h1>The duotone image.</h1>
                                    <p>This is how the image looks like after applying a duotone effect.</p>
                                </div>
                                <img src="https://i.pinimg.com/736x/33/fb/3a/33fb3a348a77bfb030926ed612495cb8.jpg" alt="Duotone">
                            </div>
                        </div>
                        <div class="handle"></div>
                    </div>
                </header>

                <div id="fh5co-couple">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-8 col-md-offset-2 text-center fh5co-heading animate-box">
