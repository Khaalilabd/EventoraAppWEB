                <script>
                    $(document).ready(function() {
                        // Gestion du slider
                        var parent = document.querySelector('.splitview'),
                            topPanel = parent ? parent.querySelector('.top') : null,
                            handle = parent ? parent.querySelector('.handle') : null,
                            skewHack = 0,
                            delta = 0;

                        if (parent) {
                            if (parent.className.indexOf('skewed') != -1) {
                                skewHack = 1000;
                            }

                            parent.addEventListener('mousemove', function(event) {
                                delta = (event.clientX - window.innerWidth / 2) * 0.5;
                                if (handle) handle.style.left = event.clientX + delta + 'px';
                                if (topPanel) topPanel.style.width = event.clientX + skewHack + delta + 'px';
                            });
                        }

                        // Gestion du formulaire Discord
                        $('#discord-message-form').submit(function(e) {
                            e.preventDefault();
                            const message = $('#message').val();
                            const errorDiv = $('#discord-message-error');

                            if (!message) {
                                errorDiv.text('Veuillez entrer un message.').show();
                                return;
                            }

                            $.ajax({
                                url: '/api/send-discord-message',
                                method: 'POST',
                                contentType: 'application/json',
                                data: JSON.stringify({ message: message }),
                                success: function(response) {
                                    errorDiv.hide();
                                    alert('Message envoyé avec succès !');
                                    $('#message').val('');
                                },
                                error: function(xhr) {
                                    errorDiv.text('Erreur : ' + (xhr.responseJSON?.error || 'Impossible d\'envoyer le message')).show();
                                }
                            });
                        });
                    });
                </script>
